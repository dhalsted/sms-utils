{
    "name": "dhalsted/sms_utils",
    "description": "Validate SMS numbers using libphonenumber and Twilio",
    "keywords": [
        "Twilio","libphonenumber","composer", "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "dhalsted",
            "email": "david.halsted@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "twilio/sdk": "^5.5",
        "giggsey/libphonenumber-for-php": "^8.8",
        "vlucas/phpdotenv": "^2.4@dev"
    },
    "require-dev" : {
        "phpunit/phpunit": " 4.8.35"    
    },
    "autoload": {
        "psr-4": {
            "Dhalsted\\SMS\\": "src/SMS",
            "Dhalsted\\SMSExceptions\\": "src/SMSexceptions"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --verbose"
    },
    "minimum-stability": "dev"

}
